{
    "name": "kanekescom/laravel-dataset",
    "description": "Drivers to handle data coming from arrays or csv",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Hadi Kurnia",
            "email": "imachmadhadikurnia@gmail.com",
            "homepage": "https://achmadhadikurnia.com"
        }
    ],
    "homepage": "https://github.com/kanekescom/laravel-dataset",
    "keywords": [
        "laravel",
        "dataset",
        "laravel-dataset"
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.10",
        "orchestra/testbench": "^6.28.0"
    },
    "autoload": {
        "psr-4": {
            "Kanekescom\\Dataset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekescom\\Dataset\\Tests\\": "tests"
        }
    },
    "suggest": {
        "kanekescom/laravel-dataset": "Required for get data",
        "kanekescom/laravel-repository-enhancer": "Required for repository enhancer"
    }
